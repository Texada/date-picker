{
  "name": "@texada/date-picker",
  "version": "0.3.0",
  "description": "A bootstrap datetime picker component for React.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Texada/react-bootstrap-datetimepicker.git"
  },
  "main": "./lib/DateTimeField.js",
  "module": "es/DateTimeField.js",
  "scripts": {
    "build": "node minifycss.js && node build/build.js",
    "build-webpack": "node minifycss.js && NODE_ENV=production webpack --output-filename react-bootstrap-datetimepicker.js",
    "examples": "webpack-dev-server --config ./examples/webpack.config.js",
    "test": "jest"
  },
  "keywords": [
    "react",
    "react-component",
    "bootstrap",
    "datetimepicker",
    "datetime"
  ],
  "author": "Lo√Øc CHOLLIER <loic@chollier.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "classnames": ">=2.0.0",
    "moment": ">=2.22.0",
    "prop-types": ">=15.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "core-js/.*",
      "<rootDir>/node_modules/react",
      "babel",
      "<rootDir>/node_modules/babel",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "setupEnvScriptFile": "<rootDir>/jestEnvironment.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathDirs": [
      "src/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "child_process": "^1.0.2",
    "classnames": "^2.1.2",
    "clean-css": "^4.2.1",
    "envify": "~3.2.0",
    "eonasdan-bootstrap-datetimepicker": "https://github.com/Eonasdan/bootstrap-datetimepicker/tarball/master",
    "grunt": "~0.4.2",
    "grunt-amd-wrap": "^1.0.1",
    "grunt-browserify": "~1.3.0",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-requirejs": "~0.4.1",
    "grunt-contrib-uglify": "~0.3.2",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-react": "~0.10.0",
    "grunt-shell": "~0.6.4",
    "gzip-size": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "moment": "^2.22.2",
    "pretty-bytes": "^5.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.2",
    "requirejs": "~2.1.9",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "react-modal": "^3.6.1"
  },
  "bugs": {
    "url": "https://github.com/Texada/react-bootstrap-datetimepicker/issues"
  },
  "directories": {
    "example": "examples",
    "lib": "lib"
  }
}
